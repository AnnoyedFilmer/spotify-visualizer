{
  "manifest_version": 3,
  "name": "Spotify Visualizer",
  "version": "1.12",
  "description": "Free Spotify visualizer + unlockable Pro features (beat + color).",
  "permissions": [
    "storage"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "icon128.png"
    }
  },
  "icons": {
    "128": "icon128.png"
  },
  "host_permissions": [
    "*://open.spotify.com/*"
  ]
}